<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/content">


  <com.spookyrobotics.graphpaper.RelativeGraphView
      android:id="@+id/graph_paper_view1"
      android:background="#FF0000"
      android:layout_width="0dp"
      android:layout_height="0dp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toStartOf="@id/graph_paper_view2"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintBottom_toTopOf="@id/graph_paper_view5"/>

    <com.spookyrobotics.graphpaper.RelativeGraphView
        android:id="@+id/graph_paper_view2"
        android:background="#FF00FF"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@id/graph_paper_view1"
        app:layout_constraintEnd_toStartOf="@id/graph_paper_view3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/graph_paper_view5"/>

    <com.spookyrobotics.graphpaper.RelativeGraphView
        android:id="@+id/graph_paper_view3"
        android:background="#FFFF00"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@id/graph_paper_view2"
        app:layout_constraintEnd_toStartOf="@id/graph_paper_view4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/graph_paper_view5"/>

    <com.spookyrobotics.graphpaper.RelativeGraphView
        android:id="@+id/graph_paper_view4"
        android:background="#00FFFF"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@id/graph_paper_view3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/graph_paper_view5"/>

    <com.spookyrobotics.graphpaper.RelativeGraphView
        android:id="@+id/graph_paper_view5"
        android:background="#FFFF00"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/graph_paper_view1"
        app:layout_constraintEnd_toEndOf="@id/graph_paper_view1"
        app:layout_constraintTop_toBottomOf="@id/graph_paper_view1"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <com.spookyrobotics.graphpaper.RelativeGraphView
        android:id="@+id/graph_paper_view6"
        android:background="#00FFFF"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/graph_paper_view2"
        app:layout_constraintEnd_toEndOf="@id/graph_paper_view2"
        app:layout_constraintTop_toBottomOf="@id/graph_paper_view1"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <com.spookyrobotics.graphpaper.RelativeGraphView
        android:id="@+id/graph_paper_view7"
        android:background="#FF0000"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/graph_paper_view3"
        app:layout_constraintEnd_toEndOf="@id/graph_paper_view3"
        app:layout_constraintTop_toBottomOf="@id/graph_paper_view1"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <com.spookyrobotics.graphpaper.RelativeGraphView
        android:id="@+id/graph_paper_view8"
        android:background="#FF00FF"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/graph_paper_view4"
        app:layout_constraintEnd_toEndOf="@id/graph_paper_view4"
        app:layout_constraintTop_toBottomOf="@id/graph_paper_view1"
        app:layout_constraintBottom_toBottomOf="parent"/>



</android.support.constraint.ConstraintLayout>
